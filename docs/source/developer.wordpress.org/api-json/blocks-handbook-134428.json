    "title": {
        "rendered": "PluginMoreMenuItem"
    },
    "content": {
        "rendered": "<p>This slot will add a new item to the More Tools &amp; Options section.</p>\n<h2>Example</h2>\n<pre><code class=\"language-js\">import { registerPlugin } from '@wordpress/plugins';\nimport { PluginMoreMenuItem } from '@wordpress/editor';\nimport { image } from '@wordpress/icons';\n\nconst MyButtonMoreMenuItemTest = () =&gt; (\n    &lt;PluginMoreMenuItem\n        icon={ image }\n        onClick={ () =&gt; {\n            alert( 'Button Clicked' );\n        } }\n    &gt;\n        More Menu Item\n    &lt;/PluginMoreMenuItem&gt;\n);\n\nregisterPlugin( 'more-menu-item-test', { render: MyButtonMoreMenuItemTest } );\n</code></pre>\n<h2>Location</h2>\n<p><img data-recalc-dims=\"1\" decoding=\"async\" src=\"https://i0.wp.com/raw.githubusercontent.com/WordPress/gutenberg/HEAD/docs/assets/plugin-more-menu-item.png?ssl=1\" alt=\"Location\" /></p>\n",
        "protected": false
    },
