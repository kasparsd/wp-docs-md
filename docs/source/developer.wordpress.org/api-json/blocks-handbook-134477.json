        "rendered": "WithSpokenMessages"
    },
    "content": {
        "rendered": "<h2>Usage</h2>\n<pre><code class=\"language-jsx\">import { withSpokenMessages, Button } from '@wordpress/components';\n\nconst MyComponentWithSpokenMessages = withSpokenMessages(\n    ( { speak, debouncedSpeak } ) =&gt; (\n        &lt;div&gt;\n            &lt;Button\n                variant=\"secondary\"\n                onClick={ () =&gt; speak( 'Spoken message' ) }\n            &gt;\n                Speak\n            &lt;/Button&gt;\n            &lt;Button\n                variant=\"secondary\"\n                onClick={ () =&gt; debouncedSpeak( 'Delayed message' ) }\n            &gt;\n                Debounced Speak\n            &lt;/Button&gt;\n        &lt;/div&gt;\n    )\n);\n</code></pre>\n",
        "protected": false
    },
